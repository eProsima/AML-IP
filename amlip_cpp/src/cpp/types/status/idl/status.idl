enum NodeKind
{
    UNDETERMINED,
    DISCOVERY,
    AGENT,
    MAIN,
    COMPUTING,
    EDGE,
    STATUS
};

enum StatusKind
{
    RUNNING,
    STOPPED,
    DROPPED,
};

struct Status
{
    AmlipId id;
    NodeKind node_kind;
    StatusKind status;
};
